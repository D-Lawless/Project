<% @events.each do |event| %>

  <div class="card">

    <%= event.title %>  <br>
    <%= event.description %>  <br>
    <%= event.location %>  <br>
    <%= event.photo %>  <br>
    <%= event.roles %>  <br>
    <%= event.start_dt %>  <br>
    <%= event.end_dt %>  <br>
    <%= event.user_id %>  <br>
    <%= event.id %>  <br>

    <%= link_to 'Show', event, class: "level-item" %>
    <%= link_to 'destroy', event, method: :delete, data: {confirm: "Are you sure you want to delete this event?"} %>

  </div>

<% end %>
